@model List<AppUser>
    <table class="table table-bordered table-striped" >
        <tr>
            <th>نام کاربر</th>
            <th>شماره موبایل</th>
            <th>وضعیت</th>            
        </tr>
        @if (Model.Any())
        {
            @foreach (var item in Model)
            {
    <tr>
        <td>@item.UserName</td>
        <td>@item.PhoneNumber</td>
        <td>@item.StatusTitle</td>

    </tr>
            }
        }
    </table>